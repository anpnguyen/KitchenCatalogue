{
  "name": "kitchen_catalogue",
  "version": "1.0.0",
  "description": "A online database for all your custom cooking recipes",
  "main": "server.js",
  "engines": {
    "node": "10.15.3",
    "npm": "6.4.1"
  },
  "scripts": {
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PleatherShaman/KitchenCatalogue.git"
  },
  "keywords": [
    "recipes",
    "database",
    "cooking"
  ],
  "author": "An Nguyen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PleatherShaman/KitchenCatalogue/issues"
  },
  "homepage": "https://github.com/PleatherShaman/KitchenCatalogue#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.12",
    "nodemailer": "^6.3.0",
    "react-select": "^3.0.4"
  },
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
